{
  "compilerOptions": {
    "target": "ES2022",
    "sourceMap": true,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "pretty": true,
    "module": "CommonJS",
    "typeRoots": [
      "@types",
      "./node_modules/@types"
    ],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
